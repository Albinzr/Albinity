nav
    height 80px
.navbar
    width: 100%
    position: fixed
    top 0
    left 0
    height 80px
    background-color: #fff
    border-bottom: 1px solid #eee;
    z-index: 1

.inner-navbar
    max-width: 950px
    width:95%
    height: 80px
    margin: 0 auto;
    position: relative;


.logo
    min-width 170px
    width 20%
    height: 80px
    float: left

.header-menu
    float: left;
    width 30%
    min-width: 265px
    margin 0 0 0 35px
    ul
        float: left;
        margin 0
        padding: 0
        list-style: none;
        li
            float: left;
            position:  relative;
            height: 80px
            margin 0 10px 0 10px
            padding 0
            font-size: 12px
            text-transform: uppercase;
            color #838383
            line-height: 80px
            font-family: 'merriweather-sans-light';
            letter-spacing: 0.33em;
            font-weight: 400
.social
    float: left;
    min-width: 85px
    width 10%
    margin: 31px 0 0 0
    li
        float left
        displa block
        margin 0 15px 0 0
        color #838383
        text-decoration: none;
.search
    float: right;
    width: 25%
    min-width: 150px
    margin 31px 0 0 0
    li:nth-child(1)
        float: left;
        display: block;
        height: 16px
        width: 16px
    li:nth-child(2) input
        // float: left
        border none
        width 110px
        padding 0
        height 17px
        border-left: 1px solid #eee;
        background-color: transparent;
        color: #838383;
        padding: 0 0 0 10px;
        font-family: 'merriweather-sans-bold';
        letter-spacing: 0.33em;
        font-size: 12px;
        font-weight: 400;
